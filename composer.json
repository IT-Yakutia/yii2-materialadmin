{
    "name": "it-yakutia/yii2-materialadmin",
    "description": "Material Admin module for Yii2",
    "type": "yii2-extension",
    "keywords": ["yii2", "extension", "materialadmin"],
    "license": "MIT",
    "authors": [
      {
        "name": "Ayaal Kaplin",
        "email": "uraankhayayaal@gmail.com"
      }
    ],
    "require": {
      "yiisoft/yii2": "~2.0.0",
      "uraankhayayaal/yii2-sortable": "*",
      "uraankhayayaal/yii2-materializecomponents": "*",
      "uraankhayayaal/yii2-redactor": "*",
      "it-yakutia/yii2-rbac": "*",
      "it-yakutia/yii2-setting": "*",
      "npm-asset/sortablejs": "^1.6.1"
    },
    "autoload": {
      "psr-4": {
        "ityakutia\\materialadmin\\": "src"
      }
    },
    "repositories": [
      {
        "type": "composer",
        "url": "https://asset-packagist.org"
      }
    ],
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
